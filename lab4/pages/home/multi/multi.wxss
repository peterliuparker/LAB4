  .background {
    width: 100%;
    height: 100%;
    position:fixed; 
    background-size:100% 100%;
    z-index: -1;
  }
  .create {
    position: absolute;
    top: 300rpx;
    
    font-size: 40rpx;
    border-radius: 20rpx;
    background-color: #343135;
    color: #e1bb7c;
  }
  .create:not([size="mini"]) {
    width: 280rpx;
    height: 90rpx;
  }
  .join {
    position: absolute;
    bottom: 550rpx;
    
    font-size: 40rpx;
    border-radius: 20rpx;
    background-color: #343135;
    color: #e1bb7c;
  }
  .join:not([size="mini"]) {
