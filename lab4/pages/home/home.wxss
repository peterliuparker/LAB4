page{
    height:100%;
  }
  .background {
    width: 100%;
    height: 100%;
    position:fixed; 
    background-size:100% 100%;
    z-index: -1;
  }
  .history {
    position: absolute;
    bottom: 330rpx;

    line-height: 47rpx;
    font-size: 30rpx;
    border-radius: 30%;

    background-color: #ca9f10;
    color: #fdfaf4;
  }
  .history:not([size="mini"]) {
    width: 230rpx;
    height: 90rpx;
  }

  .single {
    position: absolute;
    left: 66rpx;
    bottom: 130rpx;
    
    font-size: 40rpx;
    border-radius: 20rpx;
    background-color: #343135;
    color: #e1bb7c;
  }
  .single:not([size="mini"]) {
    width: 280rpx;
    height: 90rpx;
  }
  .multi {
    position: absolute;
